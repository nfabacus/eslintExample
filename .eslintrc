{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars":[
      "error",
      {
        "vars": "local",
        "args": "none",
      }
    ]
  }
}